<!DOCTYPE html>
<html lang="en">
<body>
    <header><h1>Product Management system</h1></header>
    <form action="/displayaddform">
      <button type="submit" name="btn" id="btn" value="add">Add new product</button>
    </form>
    <table border="2">
        <thead>
        <tr>
            <th>Product id</th>
            <th>Product name</th>
            <th>Product qty</th>
            <th>Product price</th>
            <th>action</th>
        </tr>
    </thead>
    <tbody>
        <%for(var i=0;i<proddata.length;i++){%>
            <tr>
                <td><%=proddata[i].pid%></td>
                <td><%=proddata[i].pname%></td>
                <td><%=proddata[i].qty%></td>
                <td><%=proddata[i].price%></td>
                <td><a href="/delete/<%=proddata[i].pid%>">delete</a>/<a href="/edit/<%=proddata[i].pid%>">edit</a></td>
            </tr>
        <%} %>
    </tbody>
    </table>
</body>
</html>